<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="cataloginventory_stock_item_save_after">
        <observer name="sfm_after_stock_item_save" instance="ShoppingFeed\Manager\Observer\AfterStockItemSaveObserver"/>
    </event>

    <event name="checkout_submit_before">
        <observer name="sfm_before_checkout_submit" instance="ShoppingFeed\Manager\Observer\BeforeCheckoutSubmitObserver"/>
